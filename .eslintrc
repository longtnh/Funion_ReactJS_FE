{
  "extends": ["react-app", "prettier"],
  "plugins": ["react", "prettier"],
  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "arrowParens": "avoid",
        "semi": false,
        "trailingComma": "all",
        "endOfLine": "auto",
        "singleQuote": true,
        "tabWidth": 2,
        "printWidth": 90,
        "useTabs": false
      }
    ],
    "no-console": "warn"
  }
}
